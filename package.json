{
  "name": "vue-filter-date-format",
  "description": "Simple datetime filter for Vue.js",
  "keywords": [
    "vue",
    "vue-filter"
  ],
  "license": "MIT",
  "version": "1.2.0",
  "author": "Eduard Nikolenko <info@eduardnikolenko.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardnikolenko/vue-filter-date-format.git"
  },
  "files": [
    "dist/vue-filter-date-format.cjs.js",
    "dist/vue-filter-date-format.esm.js"
  ],
  "main": "dist/vue-filter-date-format.cjs.js",
  "module": "dist/vue-filter-date-format.esm.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "rollup --config rollup.config.js --format cjs --file dist/vue-filter-date-format.cjs.js",
    "build:esm": "rollup --config rollup.config.js --format esm --file dist/vue-filter-date-format.esm.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ava"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.2.1",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "rollup": "^1.3.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333",
    "vue": "^2.6.7",
    "wallaby-vue-compiler": "^1.0.4"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
